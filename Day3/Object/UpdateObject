let person={

//UPDATE OBJECT PROPERTY

firstName:"Suraj",
lastName:"Bhosale",
qualification:"BE",
age:24,
skills:"Selenium webdriver with java,API,Appium,RestAssured,Postman"
}

//Accessing Object Properties

console.log(person.firstName);//Suraj
console.log(person.lastName);//Bhosale
console.log(person.qualification);//BE
console.log(person.age);//21
console.log(person.skills);//Selenium webdriver with java,API,Appium,RestAssured,Postman


//Add new Property to existing Object

person["Country"]="INDIA";
person["Height"]=5.5;
person["Interest"]="Cricket,Learning new technolgy"



console.log(person.Country);
console.log(person.Height);
console.log(person.Interest);



//Update Existing Object

person["Interest"]="Swimming";

console.log(person.Interest);
